{
  "version": 4,
  "builds": [
    {
      "src": "api/booking.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/booking",
      "dest": "/api/booking.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ]
}
