{
  "version": 2,
  "builds": [
    {
      "src": "api/booking.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/booking",
      "dest": "/api/booking.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
